; -- start job file --
[global]
rw=randwrite
size=1mb
numjobs = 4
group_reporting

;---group1(24 processes)---
[thread-seq-write1]
rw=write

[thread-seq-write2]
rw=write

[thread-seq-write3]
rw=write

[thread-seq-write4]
rw=write

;---group2(24 processes)---
[thread-rand-write1]
stonewall
rw=randwrite

[thread-rand-write2]
rw=randwrite

[thread-rand-write3]
rw=randwrite

[thread-rand-write4]
rw=randwrite

;---group3(24 processes)---
[thread-seq-read1]
stonewall
rw=read

[thread-seq-read2]
rw=read

[thread-seq-read3]
rw=read

[thread-seq-read4]
rw=read

;---group4(24 processes)---
[thread-rand-read1]
stonewall
rw=randread

[thread-rand-write1]
stonewall
rw=randread

[thread-rand-write2]
rw=randread

[thread-rand-write3]
rw=randread

[thread-rand-write4]
rw=randread

  
; -- end job file --
